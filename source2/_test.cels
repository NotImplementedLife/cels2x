package A begin
	package B begin
		multiframe function gcd(a:int, b:int):int
		begin
			while b>0 do
			begin
				var r:int;
				r=a%b;
				a=b;
				b=r;
				suspend;
			end;
			
			return a;
		end;
		
		multiframe function main():int
		begin
			var x:int = 1;
			
			var f = taskstart lambda (y:int) => (gcd(x,24));
			
			while taskready(f)==0 do begin end;
			
			var r = taskresult(f);
			
			/* taskready taskresult */
			
			while gcd(x, 24)!= 8 do
			begin
				x = x+1;
			end;
						
			return x;
		end;
	
	end;
end;